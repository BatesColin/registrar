<h1>Courses</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Enrollment Date</th>
    <th>Update</th>
    <th>Delete</th>
  </tr>
  @foreach(var course in Model)
  {
    <tr>
      <td><a href="/courses/@course.GetCourseId()">@course.GetCourseName()@course.GetCourseNumber()</a></td>
      <td>@course.GetEnrollDate()</td>
      <td><a href="/courses/@course.GetCourseId()/update">Edit</a></td>
      <td><a href="/courses/@course.GetCourseId()/delete">Delete</a></td>
    </tr>
  }
</table>
<p><a href="/courses/new">Add a New Course</a></p>
<p><a href="/">Back to Home</a></p>
